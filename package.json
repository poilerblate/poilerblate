{
  "name": "poilerblate",
  "version": "0.6.1",
  "description": "React boilerplate",
  "main": "index.js",
  "repository": "github:poilerblate/poilerblate",
  "bugs": {
    "url": "https://github.com/poilerblate/poilerblate/issues"
